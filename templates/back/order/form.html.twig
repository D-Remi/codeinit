{{ form_start(formOrder) }}
<div class="form-group">
    {{ form_label(formOrder.email, "Email", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
    <span class="text-danger">{{ form_errors(formOrder.email) }}</span>
    <div class="col-sm-10">
        {{ form_widget(formOrder.email , {'attr': {'class': 'form-control'}}) }}
    </div>
</div>
<div class="form-group">
    {{ form_label(formOrder.startDate, "Date de debut", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
    <span class="text-danger">{{ form_errors(formOrder.startDate) }}</span>
    <div class="col-sm-10">
        {{ form_widget(formOrder.startDate , {'attr': {'class': 'input-inut'}}) }}
    </div>
</div>

<button type="submit" class="btn btn-primary">Valider</button>
<a type="reset" href="{{ path( 'app_admin_user' ) }}" class="btn btn-danger">Retour</a>
{{ form_end(formOrder) }}